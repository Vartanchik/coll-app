                        "schema": {
                            "type": "float"
                        }
                    },
                    {
                        "name": "remainingAmount[ne]",
                        "in": "query",
                        "description": "Not equal. You need choose only one of remainingAmount filtering parameter!",
                        "required": false,
                        "schema": {
                            "type": "float"
                        }
